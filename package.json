{
  "name": "betpoa-backend",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "_moduleAliases": {
    "@controllers": "dist/controllers",
    "@hooks": "dist/hooks",
    "@configs": "dist/configs",
    "@rapidapi": "dist/rapidapi",
    "@cronjobs": "dist/cronjobs",
    "@utils": "dist/utils"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && c8 node --test -r ts-node/register test/**/*.ts",
    "start": "fastify start -l info dist/app.js",
    "build": "tsc ",
    "build:ts": "tsc && yarn sentry:sourcemaps",
    "watch:ts": "tsc -w",
    "dev": "npm run build && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js",
    "generate": "prisma generate",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org betpoa --project node dist && sentry-cli sourcemaps upload --org betpoa --project node dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/sensible": "^5.0.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/type-provider-typebox": "^4.0.0",
    "@immobiliarelabs/fastify-sentry": "^8.0.1",
    "@prisma/client": "5.11.0",
    "@scalar/fastify-api-reference": "^1.20.10",
    "@sentry/cli": "^2.31.0",
    "@sentry/profiling-node": "^7.109.0",
    "@sinclair/typebox": "^0.32.20",
    "@types/africastalking": "^0.6.2",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "africastalking": "^0.6.6",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "cron": "^3.1.6",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.1",
    "fastify-cli": "^6.1.1",
    "fastify-html": "^0.3.3",
    "fastify-plugin": "^4.0.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.4.4",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "c8": "^9.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "fastify-tsconfig": "^2.0.0",
    "prettier": "^3.2.5",
    "prisma": "^5.11.0",
    "ts-node": "^10.4.0",
    "typescript": "^5.2.2"
  }
}
